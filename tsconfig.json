{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./app_package/static/ts/",
    "outDir": "./app_package/static/gen/",
    "removeComments": true,
    "noEmitOnError": true, 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
